{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
		"es6": true
    },
    "extends": "eslint:recommended",
	"parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false,
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
		"no-mixed-spaces-and-tabs": "off"
    }
}
